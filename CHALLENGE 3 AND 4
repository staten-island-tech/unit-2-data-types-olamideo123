#Changle 3 printing out factors   

def print_factors(x):  
     print("The factors of" ,x,"are:") 
     for i in range(1, x +1): 
          if(x % i == 0): 
               print(i)


number = 45
print_factors(number) 

#code for greatest common factor   Changle 4   GCF data types assignment

def gcf(x,y): 
    if(y == 0): 
         return abs(x) 
    else: 
          return gcf(y, x % y) 


x = 16
y = 28 


print(gcf(16,28))